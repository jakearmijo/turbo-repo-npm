{
  "name": "@repo/node",
  "version": "3.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "scripts": {
    "test": " NODE_ENV=test jest",
    "dev": "nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/app.ts",
    "test:anonymous": "nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/app.ts",
    "test:registered": "nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/app.ts",
    "start:prod": "npm run build && node --es-module-specifier-resolution=node dist/app.js",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "eslint ./src/**/*.ts",
    "build:original": "tsc --noEmit false",
    "build": "esbuild src/app.ts --bundle --platform=node --format=esm --outfile=dist/bundle.js --external:mock-aws-s3 --external:nock"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.465.0",
    "@aws-sdk/client-secrets-manager": "^3.437.0",
    "@aws-sdk/client-sns": "^3.378.0",
    "@propelauth/express": "^2.1.4",
    "@sendgrid/mail": "^8.1.1",
    "@types/response-time": "^2.3.8",
    "ably": "^1.2.48",
    "axios": "1.6.1",
    "body-parser": "^1.20.2",
    "colors": "^1.4.0",
    "common": "*",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "path": "^0.12.7",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "prom-client": "^15.0.0",
    "redis": "^4.6.10",
    "response-time": "^2.3.2",
    "sequelize": "^6.31.1",
    "sequelize-typescript": "^2.1.5",
    "snowflake-sdk": "^1.10.0",
    "@repo/types": "*",
    "unique-names-generator": "^4.7.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@faker-js/faker": "^8.3.1",
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.1",
    "@types/sequelize": "^4.28.16",
    "@types/snowflake-sdk": "^1.6.20",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/parser": "^6.13.2",
    "esbuild": "0.20.2",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^13.2.3",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
